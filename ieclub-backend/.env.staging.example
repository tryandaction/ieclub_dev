# Staging Environment Configuration (娴嬭瘯鐜)
# 鐢ㄤ簬鍐呴儴娴嬭瘯锛屼笉褰卞搷绾夸笂鐢ㄦ埛

NODE_ENV=staging
PORT=3001

# 鏁版嵁搴撻厤缃?- 浣跨敤娴嬭瘯鏁版嵁搴?DATABASE_URL=mysql://ieclub_user:ieclub_password_2024@localhost:3306/ieclub_staging

# Redis 閰嶇疆
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=1

# JWT 閰嶇疆锛堟祴璇曠幆澧冨彲浠ヤ娇鐢ㄧ浉瀵圭畝鍗曠殑瀵嗛挜锛?JWT_SECRET=staging_jwt_secret_key_min_32_characters_long_for_testing
JWT_REFRESH_SECRET=staging_jwt_refresh_secret_key_min_32_characters_long_for_testing
JWT_EXPIRES_IN=7d
JWT_REFRESH_EXPIRES_IN=30d
SESSION_SECRET=staging_session_secret_key_min_32_characters_long_for_testing

# CORS 閰嶇疆
CORS_ORIGIN=https://test.ieclub.online,http://localhost:5173

# 鍓嶇鍦板潃
FRONTEND_URL=https://test.ieclub.online

# 寰俊灏忕▼搴忛厤缃紙娴嬭瘯鐜锛?WECHAT_APPID=your_test_appid
WECHAT_SECRET=your_test_secret

# 鏂囦欢涓婁紶閰嶇疆
MAX_FILE_SIZE=10485760
UPLOAD_DIR=./uploads/staging

# 鏃ュ織閰嶇疆
LOG_LEVEL=debug
LOG_DIR=./logs/staging

# 缂撳瓨閰嶇疆
CACHE_TTL=300
CACHE_ENABLED=true

# 闄愭祦閰嶇疆锛堟祴璇曠幆澧冨彲浠ユ洿瀹芥澗锛?RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=200

# 閭欢閰嶇疆锛堟祴璇曠幆澧冿級
EMAIL_HOST=smtp.example.com
EMAIL_PORT=587
EMAIL_USER=test@ieclub.online
EMAIL_PASSWORD=your_test_email_password
EMAIL_FROM=IEClub Test <test@ieclub.online>

# 鐩戞帶閰嶇疆
ENABLE_MONITORING=true
ENABLE_ALERTS=false
